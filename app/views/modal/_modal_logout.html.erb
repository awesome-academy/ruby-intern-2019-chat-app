<div class="modal fade" id="confirm-delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">

      <div class="modal-header">
        <h4 class="modal-title"><%= t("modal.modal_log_out.title") %></h4>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>

      <div class="modal-body">
        <p><%= t("modal.modal_log_out.warning") %></p>
        <p class="debug-url"></p>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"><%= t("modal.cancel") %></button>
        <%= link_to t("modal.close"),destroy_user_session_path, :method => :delete, class: "btn btn-danger btn-ok" %>
      </div>
    </div>
  </div>
</div>
